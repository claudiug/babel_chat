<div class="row">
  <h2>Hello <%= session[:username].capitalize %></h2>
  <p>You selected language:<strong><%= session[:type] %></strong></p>
</div>
<div id="chat">

</div>


<%= form_tag messages_path, method: :post, remote:true do %>
    <%= text_field_tag :message %>
    <%= submit_tag 'send', id: 'send' %>
<% end %>
